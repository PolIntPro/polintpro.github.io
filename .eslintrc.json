{
    "plugins": ["compat"],
    "extends": [
        "plugin:compat/recommended",
        "next/core-web-vitals",
        "next/typescript"
    ],
    "rules": {
        "semi": "off",
        "@typescript-eslint/no-unused-vars": [
            "warn",
            {
                "ignoreRestSiblings": true
            }
        ],
        "@typescript-eslint/no-explicit-any": "warn",
        "@typescript-eslint/no-empty-object-type": "warn"
    },
    "env": {
        "browser": true
    }
}
